pipeline {
  agent none
  stages {
        stage('Install') {
          agent {
            docker { image 'node:16-alpine' }
            }
            steps {
               sh 'npm install'
            }
        }    
        stage('Run Application') {
          agent {
            docker { image 'node:16-alpine' }
            }
            steps {
                sh 'npm start'
            }
        }
    
        stage ('Build') {
            steps {
                sh 'javac HelloWorld.java'
            }
        }
        stage ('Run') {
            steps {
                sh 'java HelloWorld'
            }
        }
        stage('DB') {
          agent {
            docker { image 'mysql:latest' }
            }
      steps {
        sh 'touch new_txt_file.txt'
      }
    }
  }
}
